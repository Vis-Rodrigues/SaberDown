@model IList<SDW.WebServiceJogo.MVC.Models.Categoria>
@{
    ViewBag.Title = "Listar";
}
@Helper.Mensagem(@TempData["msg"])
<h2>Listar</h2>
<div class="table table-striped">
    <table>
        <tr>
            <th>Codigo</th>
            <th>Descricao</th>
        </tr>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.CategoriaId</td>
                <td>@item.Descricao</td>
            </tr>
        }
    </table>
</div>

